{
  "env": {
    "es6": true,
    "browser": true
  },
  "extends": ["airbnb", "plugin:react/recommended"],
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    }
  },
  "rules": {
    "no-console": 1,
    "linebreak-style": 0,
    "react/prefer-stateless-function": 0,
    "react/jsx-filename-extension": 0,
    "comma-dangle": 0,
    "jsx-a11y/anchor-is-valid": 0,
    "react/prop-types": 1,
    "import/no-extraneous-dependencies": 1,
    "import/no-unresolved": 1,
    "import/extensions": 1,
    "no-debugger": 1,
    "max-len": 1,
    "no-nested-ternary": 1,
    "no-unused-vars": 1,
    "react/no-unused-state": 1
  },
  "settings": {
    "import/resolver": {
      "node": {
        "paths": ["src"]
      }
    }
  }
}
